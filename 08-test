const arr = [3, 5, 1, 3,]


function add(a) {
    return a * 5;
}

Array.prototype.cb = function (callback) {
    for (let i = 0; i < this.length; i++) {
        callback(this[i])

    }
}
arr.cb((r) => console.log(r))
